@model List<CivilCalc.DAL.MST.MST_Configuration.SelectAll_Result>

<div class="card bg-light shadow-sm mt-5">
    <div class="card-header">
        <h3 class="card-title">Title</h3>
        <div class="card-toolbar">            

            <button class="btn btn-sm btn-success js-stkModal-btn ms-1"
                    data-form-id="formAdd_UMS"
                    data-url='@Url.Action("_AddEdit", "MST_Configuration", new {Area = "MST_Configuration"})'
                    data-modal-size="lg">
                <i class="la la-plus"></i> Add New
            </button>
        </div>
    </div>
    <div class="card-body">
        <table class="table table-bordered table-hover" id="tblList" cellspacing="0" width="100%">
            <thead class="bg-gray-300">
                <tr>
                    <th class="text-center">Sr.</th>
                    <th class="">WebsiteLogoPath</th>
                    @*<th class="">MetaTitle</th>
                    <th class="">MetaKeyword</th>
                    <th class="">MetaDescription</th>
                    <th class="">MetaAuthor</th>
                    <th class="">MetaOgTitle</th>
                    <th class="">MetaOgImage</th>
                    <th class="">MetaOgDescription</th>
                    <th class="">MetaOgUrl</th>
                    <th class="">MetaOgType</th>*@
                    <th class="">AboutWebsite</th>
                    <th class="">MobileAppDetail</th>
                    <th class="">PlayStoreURL</th>
                    <th class="">AppStoreURL</th>
                    <th class="">FooterHTML</th>
                    <th class="">Description</th>
                    <th class="">UserID</th>
                    <th class="text-center">Actions</th>
                </tr>
            </thead>
            <tbody>
                @{
                    int i = 0;
                }
                @foreach (var item in Model)
                {
                    i++;
                    <tr>
                        <td class="text-center"><span data-toggle="kt-tooltip" data-placement="right" data-original-title="">@item.ConfigurationID</span></td>
                       
                        <td class="">@item.WebsiteLogoPath</td>
                      @*  <td class="">@item.MetaTitle</td>
                        <td class="">@item.MetaKeyword</td>
                        <td class="">@item.MetaDescription</td>
                        <td class="">@item.MetaAuthor</td>
                        <td class="">@item.MetaOgTitle</td>
                        <td class="">@item.MetaOgImage</td>
                        <td class="">@item.MetaOgDescription</td>
                        <td class="">@item.MetaOgUrl</td>
                        <td class="">@item.MetaOgType</td>*@
                        <td class="">@item.AboutWebsite</td>
                        <td class="">@item.MobileAppDetail</td>
                        <td class="">@item.PlayStoreURL</td>
                        <td class="">@item.AppStoreURL</td>
                        <td class="">@item.FooterHTML</td>
                        <td class="">@item.Description</td>
                        <td class="">@item.UserName</td>

                        <td class="text-center">

                            <span data-bs-toggle="tooltip" data-bs-placement="left" title="Edit">
                                <button class="btn btn-primary btn-icon btn-elevate btn-sm js-stkModal-btn"
                                    data-form-id="formEdit_UMS"
                                    data-url='@Url.Action("_AddEdit", "MST_Configuration", new {Area = "MST_Configuration", ConfigurationID = item.ConfigurationID})'
                                    data-modal-size="lg">
                                    <i class="la la-edit"></i>
                                </button>
                            </span>

                            <form id="formDelete_UMS" data-ajax="true" data-ajax-method="post" data-ajax-mode="replace" data-ajax-update="#SearchResult"
                              data-ajax-success="OnSuccess(this.id,data, status, xhr);" data-ajax-failure="OnFailure(xhr, status, error);"
                              asp-area="MST_Configuration" asp-action="_Delete" asp-controller="MST_Configuration" style="display: inline;">
                                @Html.AntiForgeryToken()
                                <input type="hidden" name="ConfigurationID" value="@item.ConfigurationID" />
                                <a class="btn btn-danger btn-icon btn-elevate btn-sm js-bb-confirm-delete" data-bs-toggle="tooltip" data-bs-placement="left" title="Delete"><i class="la la-trash"></i></a>
                            </form>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>