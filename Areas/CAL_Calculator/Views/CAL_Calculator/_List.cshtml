@model List<CivilCalc.DAL.CAL.CAL_Calculator.SelectByCategoryNameCalculatorNameUserName_Result>

<div class="card bg-light shadow-sm mt-5">
    <div class="card-header">
        <h3 class="card-title">Title</h3>
        <div class="card-toolbar">

            <button class="btn btn-sm btn-success js-stkModal-btn ms-1"
                    data-form-id="formAdd_UMS"
                    data-url='@Url.Action("_AddEdit", "CAL_Calculator", new {Area = "CAL_Calculator"})'
                    data-modal-size="lg">
                <i class="la la-plus"></i> Add New
            </button>
        </div>
    </div>
    <div class="card-body">
        <table class="table table-responsive table-bordered table-hover " id="tblList" cellspacing="0" width="100%">
            <thead class="bg-gray-300">
                <tr>
                    <th class="text-center">Sr.</th>
                    <th class="text-right">Calculator</th>
                    <th class="">Calculator Name </th>
                    <td class="">CalculatorIcon</td>
                    <td class="">URLName</td>
                    <td class="">HeaderName</td>
                    <td class="">SubHeaderName</td>
                    <td class="">CalculatorDescription</td>
                    <td class="">PageSection1</td>
                    <td class="">PageSection2</td>
                    <td class="">PageSection3</td>
                    <td class="">MetaTitle</td>
                    <td class="">MetaKeyword</td>
                    <td class="">MetaDescription</td>
                    <td class="">MetaAuthor</td>
                    <td class="">MetaOgTitle</td>
                    <td class="">MetaOgImage</td>
                    <td class="">MetaOgDescription</td>
                    <td class="">MetaOgUrl</td>
                    <td class="">MetaOgType</td>
                    <td class="">Description</td>
                    <td class="">Sequence</td>
                    <td class="">UserID</td>
                    <th class="text-center">Actions</th>
                </tr>
            </thead>
            <tbody>
                @{
                    int i = 0;
                }
                @foreach (var item in Model)
                {
                    i++;
                    <tr>
                        <td class="text-center"><span data-toggle="kt-tooltip" data-placement="right" data-original-title="">@i</span></td>
                        <td class="text-right">@item.CalculatorID</td>
                        <td class="text-right">@item.CategoryName</td>
                        <td class="">@item.CalculatorName</td>
                        <td class="">@item.CalculatorIcon</td>
                        <td class="">@item.URLName</td>
                        <td class="">@item.HeaderName</td>
                        <td class="">@item.SubHeaderName</td>
                        <td class="">@item.CalculatorDescription</td>
                        <td class="">@item.PageSection1</td>
                        <td class="">@item.PageSection2</td>
                        <td class="">@item.PageSection3</td>
                        <td class="">@item.MetaTitle</td>
                        <td class="">@item.MetaKeyword</td>
                        <td class="">@item.MetaDescription</td>
                        <td class="">@item.MetaAuthor</td>
                        <td class="">@item.MetaOgTitle</td>
                        <td class="">@item.MetaOgImage</td>
                        <td class="">@item.MetaOgDescription</td>
                        <td class="">@item.MetaOgUrl</td>
                        <td class="">@item.MetaOgType</td>
                        <td class="">@item.Description</td>
                        <td class="">@item.Sequence</td>
                        <td class="">@item.UserID</td>
                        <td class="text-center">

                            <span data-bs-toggle="tooltip" data-bs-placement="left" title="Edit">
                                <button class="btn btn-primary btn-icon btn-elevate btn-sm js-stkModal-btn"
                                    data-form-id="formEdit_UMS"
                                    data-url='@Url.Action("_AddEdit", "CAL_Calculator", new {Area = "CAL_Calculator", CalculatorID = item.CalculatorID})'
                                    data-modal-size="lg">
                                    <i class="la la-edit"></i>
                                </button>
                            </span>

                            <form id="formDelete_UMS" data-ajax="true" data-ajax-method="post" data-ajax-mode="replace" data-ajax-update="#SearchResult"
                              data-ajax-success="OnSuccess(this.id,data, status, xhr);" data-ajax-failure="OnFailure(xhr, status, error);"
                              asp-area="CAL_Calculator" asp-action="_Delete" asp-controller="CAL_Calculator" style="display: inline;">
                                @Html.AntiForgeryToken()
                                <input type="hidden" name="CalculatorId" value="@item.CalculatorID" />
                                <a class="btn btn-danger btn-icon btn-elevate btn-sm js-bb-confirm-delete" data-bs-toggle="tooltip" data-bs-placement="left" title="Delete"><i class="la la-trash"></i></a>
                            </form>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>