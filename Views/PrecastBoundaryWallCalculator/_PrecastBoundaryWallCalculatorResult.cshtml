               <!--begin: table -->
                    <div class="col-md-6">
                        <div class="alert alert-block alert-info">
                            <strong>Note:&nbsp;</strong>No Space is kept between horinontal bars
                        </div>
                <div class="table-responsive">
                 <table class="table table-row-bordered">
                    <thead>
                      <tr class="fw-bolder fs-6 text-gray-800">
                        <th>Sr.</th>
                        <th>Material</th>
                                        <th>Quantity</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>1</td>
                                        <td>Horizontal Bar</td>
                                        <td>@ViewBag.lblAnswerHorizontalBarB</td>
                      </tr>
                      <tr>
